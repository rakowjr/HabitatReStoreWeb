<%@ Master Language="C#" AutoEventWireup="true" CodeFile="DonorMasterPage.master.cs" Inherits="DonorMasterPage" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title></title>
    <meta charset="utf-8" />
    <link href="styles/StyleSheet.css" rel="stylesheet" />
    <asp:ContentPlaceHolder id="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="form1" runat="server">
    <div id="container">
        <header class="wrapper">
            <div style="height: 57px">
                <div id="logo">
                    <asp:Image ID="Image1" runat="server" ImageUrl="~/images/logo.png" />
                </div>
            </div>
            <nav class="clearfix">
                <ul>
<li>
                        <asp:HyperLink ID="HyperLink1" runat="server" NavigateUrl="#">About Us</asp:HyperLink></li>
                    <li>
                        <asp:HyperLink ID="HyperLink2" runat="server" NavigateUrl="~/Default.aspx">ReStores</asp:HyperLink></li>
                    <li>
                        <asp:HyperLink ID="HyperLink3" runat="server" NavigateUrl="~/Volunteers.aspx">Volunteer</asp:HyperLink></li>
                    <li>
                        <asp:HyperLink ID="HyperLink4" runat="server" NavigateUrl="~/NewDonor.aspx">Donate</asp:HyperLink></li>
                </ul>
            </nav>
        </header>
        <div id="breadCrumbContainer">
            <asp:SiteMapPath ID="SiteMapPath2" runat="server" CssClass="breadCrumb"></asp:SiteMapPath>
        </div>
        <section id="page" class="clearfix">
            <div>
                <asp:ContentPlaceHolder id="ContentPlaceHolder1" runat="server">        
                </asp:ContentPlaceHolder>
            </div>
        </section>
        <aside id="sideBar">
            <div style="margin-left: 60px;">
            <div style="margin-top: 50px;">
                <h3>Recently Donated Items</h3>
            </div>
            <div class="sideBarImage">
                <asp:Image ID="Item1" runat="server" CssClass="itemImage"/>
            </div>
            <div class="sideBarImage">
                <asp:Image ID="Item2" runat="server" CssClass="itemImage" />
            </div>
            <div class="sideBarImage">
                <asp:Image ID="Item3" runat="server" CssClass="itemImage" />
            </div>
            <div class="sideBarImage">
                <asp:Image ID="Item4" runat="server" CssClass="itemImage" />
            </div>
            </div>
        </aside>
        <footer>
            <div></div>
        </footer>
    </div>
    </form>
</body>
</html>
